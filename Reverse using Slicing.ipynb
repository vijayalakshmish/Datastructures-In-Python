   "source": [
    "#Reverse using Slicing\n",
    "import numpy as np\n",
    "x = np.array([5,10,20,0,3,6])\n",
    "print(x[::-1])\n",
    "\n",
    "#Reverse using Loop\n",
    "n = len(x)\n",
    "for i in range(n-1,-1,-1):\n",
    "    print(x[i])"
   ]
