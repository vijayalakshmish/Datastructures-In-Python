   "source": [
    "#function to rotate an array to the right by k positions\n",
    "\n",
    "def rotate(arr , k):\n",
    "    n = len(arr)\n",
    "    k = k%n\n",
    "    return arr[-k:] + arr[:-k]\n",
    "\n",
    "arr = [1,2,3,4,5]\n",
    "k = 2\n",
    "rotated = rotate(arr,k)\n",
    "print(rotated)"
   ]
